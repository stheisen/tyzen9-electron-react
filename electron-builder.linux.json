{
    "appId": "com.tyzen9.electronReact",
    "productName": "Tyzen9-electron-react",
    "files": [
        "build/**/*",
        "app/electron/**/*",
        "node_modules/**/*",
        {
            "from": "resources/icons",
            "to": "build/icons",
            "filter": ["**/*.{png}"]
